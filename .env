# MIA Environment Configuration
# Copy this to .env and update values as needed

# Application
MIA_DEBUG=true
MIA_HOST=0.0.0.0
MIA_PORT=8000

# AI Model
MIA_MODEL_PATH=weights/best_mia.pt
MIA_CONFIDENCE=0.5
MIA_DEVICE=auto

# Database
MIA_DATABASE_URL=sqlite+aiosqlite:///./data/mia_violations.db

# Roboflow (for downloading training data)
# Get your key at: https://app.roboflow.com/settings/api
ROBOFLOW_API_KEY=YOUR_KEY_HERE
